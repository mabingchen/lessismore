# From proc folder
# cpu/mem/hd/networkcard

# cat /proc/
root@mbc-ubuntu-jenkins:~# cat /proc/cpuinfo
root@mbc-ubuntu-jenkins:~# cat /proc/meminfo

# From cmd
root@mbc-ubuntu-jenkins:~# lscpu
[root@mbc-centos73 ~]# dmidecode -t memory  bios/system/baseboard/chassis/processor/memory/cache/connector/slot
root@mbc-ubuntu-jenkins:~# fdisk -l
root@mbc-ubuntu-jenkins:~# ifconfig
[root@mbc-centos73 ~]# lspci

# hd

[root@mbc-centos73 ~]# cat /etc/fstab
LABEL=rootdisk  /          xfs      defaults,noatime                            1 1
devpts          /dev/pts   devpts   rw,nosuid,noexec,relatime,gid=5,mode=620    0 0
tmpfs           /dev/shm   tmpfs    defaults                                    0 0
proc            /proc      proc     defaults                                    0 0
sysfs           /sys       sysfs    defaults                                    0 0

